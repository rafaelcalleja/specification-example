{
    "name": "rc/jsonrpc",
    "type": "library",
    "description": "Library for Json RPC 2.0 Specification",
    "homepage": "https://github.com/rafaelcalleja/jsonrpc-spec",
    "keywords": ["json", "rpc", "jsonrpc", "specification", "2.0", "json-rpc"],
    "license": "MIT",
    "authors": [
      {
        "name": "Rafael Calleja",
        "email": "rafaelcalleja@gmail.com",
        "homepage": "https://github.com/rafaelcalleja"
      }
    ],
    "require": {
      "doctrine/orm": "^2.0",
        "ext-pdo_sqlite": "^1.0"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
	        "Example\\": "src/Example"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Example\\Tests\\": "tests/Example"
        }
    },
    "config": {
      "bin-dir": "bin",
       "platform": {
        "php": "5.5"
       }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
